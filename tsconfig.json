{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "store/*": ["src/app/store/*"],
      "app/*": ["src/app/*"],
    },
    "sourceMap": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "target": "es5",
    "jsx": "react",
    "lib": [
      "dom",
      "es6"
    ],
  },
  "include": [
    "src"
  ],
  "compileOnSave": false
}
